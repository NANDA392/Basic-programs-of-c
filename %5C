#include<stdio.h>
int main()
{
    int a[3][3],i,j;
    int sum=0;
    printf("enter the elemnts of matrix 3x3\n");
    for(i=0;i<3;i++)
        for(j=0;j<3;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            if(i==j)
                sum=sum+a[i][j];
        }
    }
    printf("SUM of diagonal elemants = %d\n",sum);
}
